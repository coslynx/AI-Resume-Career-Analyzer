# OpenAI API Key for generating resume feedback
OPENAI_API_KEY=YOUR_OPENAI_API_KEY_HERE

# Stripe Secret Key for secure payment processing
STRIPE_SECRET_KEY=YOUR_STRIPE_SECRET_KEY_HERE

# MongoDB Atlas connection string for storing user data and resumes
MONGODB_URI=mongodb+srv://your-username:your-password@your-cluster.abcde.mongodb.net/your-database?retryWrites=true&w=majority

# JSON Web Token (JWT) secret key for user authentication
JWT_SECRET=your_jwt_secret_key_here

# Node.js environment (development, staging, production)
NODE_ENV=development

# Application server port
PORT=3000

# Base URL of the application for API calls and routing
REACT_APP_BASE_URL=http://localhost:3000